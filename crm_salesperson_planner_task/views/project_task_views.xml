<odoo>
    <record id="crm_salesperson_planner_task_view_form" model="ir.ui.view">
        <field name="name">crm.salesperson.planner.task.view.form</field>
        <field name="model">project.task</field>
        <field name="inherit_id" ref="project.view_task_form2" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='date_deadline']" position="after">
                <field name="visit_id" />
            </xpath>
        </field>
    </record>
</odoo>
